<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lunodzo.meetinglite">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_calendar_today_24"
        android:label="@string/app_name"
        android:logo="@drawable/ic_baseline_calendar_today_24"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MeetingLite">
        <activity
            android:name=".MeetingDesc"
            android:label="Meeting Description"
            android:parentActivityName=".MeetingList" />
        <activity
            android:name=".SetMeeting"
            android:label="Set Meeting"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MeetingList"
            android:label="Meeting List"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>